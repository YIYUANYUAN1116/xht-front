      "post": {
        "tags": [
          "工作流接口"
        ],
        "summary": "部署",
        "operationId": "deploy",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "required": [
                  "file"
                ],
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary",
                    "name": "file",
                    "in": "query"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/Result"
                }
              }
            }
          }
        }
      }
    },
    "/activiti/apply": {
      "post": {
        "tags": [
          "工作流接口"
        ],
        "summary": "开启一个流程",
